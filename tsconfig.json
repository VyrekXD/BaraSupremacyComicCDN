{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"strictFunctionTypes": true,
		"noImplicitReturns": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
	},
	"include": [
		"./src"
	],
	"exclude": [
		"./node_modules",
		"./Images"
	]
}